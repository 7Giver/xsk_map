#app {
  padding-bottom: 160rpx;
}
#app .banner {
  background: #fff;
}
#app .banner::after {
  content: '';
  display: block;
  position: absolute;
  width: 0;
  height: 0;
  border-right: 20rpx solid transparent;
  border-left: 20rpx solid transparent;
  border-top: 20rpx solid #222841;
  margin-left: 54rpx;
}
#app .banner > image {
  width: 100%;
  vertical-align: middle;
}
#app .content .bg_box {
  padding: 50rpx 50rpx 0;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 80%;
  background-position: -12% 68%;
}
#app .content .bg_box1 {
  padding: 50rpx 50rpx 0;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 80%;
  background-position: -12% 203%;
}
#app .content .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 70rpx;
  color: #5B68AC;
  margin-bottom: 30rpx;
  position: relative;
}
#app .content .title .num {
  width: 60rpx;
  height: 60rpx;
  font-size: 38rpx;
  line-height: 60rpx;
  text-align: center;
  margin-right: 30rpx;
  border: 1px solid #5B68AC;
  border-radius: 14rpx;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
}
#app .content .title .num > text {
  display: block;
  -webkit-transform: rotate(-45deg);
          transform: rotate(-45deg);
}
#app .content .title > text {
  letter-spacing: 1.5px;
}
#app .content .message {
  font-size: 30rpx;
  line-height: 48rpx;
}
#app .content .message > text {
  color: #FC645F;
}
#app .content .text_box .text_item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
  position: relative;
}
#app .content .text_box .text_item:not(:last-child) {
  margin-bottom: 50rpx;
}
#app .content .text_box .text_item .tip {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #5B68AC;
  font-size: 34rpx;
}
#app .content .text_box .text_item .tip::after {
  content: '';
  display: block;
  width: 12rpx;
  height: 12rpx;
  margin: 0 20rpx;
  background-color: #FC645F;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
}
#app .content .text_box .text_item:not(:last-child)::before {
  content: '';
  position: absolute;
  bottom: -52rpx;
  display: block;
  width: 45rpx;
  height: 45rpx;
  margin: 10rpx 20rpx;
  border-left: 1px solid #5B68AC;
  opacity: 0.3;
}
#app .content .text_box .text_item:nth-child(4)::before {
  content: '';
  height: 96rpx;
}
#app .content .text_box .text_item > text {
  color: #333537;
  font-size: 30rpx;
  line-height: 50rpx;
  font-family: PingFangSC-Regular;
}

